{
  "name": "react-keystone",
  "version": "2.0.2",
  "description": "React components for the Pennsylvania Keystone Design System",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external react-dom --inject-style",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react --external react-dom --inject-style",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "components",
    "keystone",
    "pennsylvania",
    "design-system",
    "kds"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
